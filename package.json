  "version": "1.0.0-alpha.1",
  "description": "eslint-config-standard for megalo project",
  "main": "./lib/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint --fix lib/**/*.js"
